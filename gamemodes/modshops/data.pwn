stock SOLS_GetComponentModelName(componentid, name[], size = sizeof(name)) {

	for ( new i, j = sizeof ( SOLS_Component_Stack ); i < j ; i ++ ) {

		if ( componentid == SOLS_Component_Stack [ i ] [ E_SOLS_TUNEDATA_COMP ] ) {

			strunpack(name, SOLS_Component_Stack [ i ] [ E_SOLS_TUNEDATA_NAME ]);
			break ;
		}
		else continue ;
	}
}

SOLS_GetComponentType(componentid) {

	for ( new i, j = sizeof ( SOLS_Component_Stack ); i < j ; i ++ ) {

		if ( componentid == SOLS_Component_Stack [ i ] [ E_SOLS_TUNEDATA_COMP ] ) {

			return SOLS_Component_Stack [ i ] [ E_SOLS_TUNEDATA_TYPE ] ;
		}

		else continue ;
	}

	return E_COMPONENT_TYPE_INVALID ;
}

SOLS_GetComponentTypeName(component_type, name[], size = sizeof(name)) {
	for ( new i, j = sizeof ( SOLS_Component_Types ); i < j ; i ++ ) {

		if ( component_type == SOLS_Component_Types [ i ] [ E_COMPONENT_TYPE_CONST ] ) {

			strunpack(name, SOLS_Component_Types [ i ] [ E_COMPONENT_TYPE_DESC ], size);
			break ;
		}
		else continue ;
	}
}

stock GetVehicleComponentName(componentid, name[], size = sizeof(name))
{
	new idx = componentid - 1000;

	if (!(0 <= idx <= MAX_VEHICLES_COMPONENTS)) {
		return 0;
	}
	return strunpack(name, gVehicleComponentName[idx], size);
}

stock GetVehicleComponentCost(componentid)
{
	new idx = componentid - 1000;

	if (!(0 <= idx <= MAX_VEHICLES_COMPONENTS)) {
		return 0;
	}
	return gVehicleComponentCost[idx];
}

stock IsVehicleUpgradeCompatible(modelid, componentid)
{
	new idx = modelid - 400;

	if (!(0 <= idx <= MAX_VEHICLES_MODELS)) {
		return 0;
	}
	for (new i; i < MAX_VEHICLE_COMPONENTS; i++)
	{
		if (gVehicleComponents[idx][i] != INVALID_VEHICLE_COMPONENT_ID && gVehicleComponents[idx][i] == componentid) {
			return 1;
		}
	}
	return 0;
}

stock GetVehicleCompatibleUpgrades(modelid, dest[], size = sizeof(dest), &count = 0)
{
	new idx = modelid - 400;

	if (!(0 <= idx <= MAX_VEHICLES_MODELS)) {
		return 0;
	}
	count = 0;

	for (new i; i < MAX_VEHICLES_COMPONENTS; i++)
	{
		if (i < size)
		{
			if (gVehicleComponents[idx][i] == INVALID_VEHICLE_COMPONENT_ID) {
			 	break;
			}
			dest[count++] = gVehicleComponents[idx][i];
		}
	}
	return 1;
}

stock DoesVehicleHaveUpgrades(modelid)
{
	new idx = modelid - 400;

	if (!(0 <= idx <= MAX_VEHICLES_MODELS)) {
		return 0;
	}
	return (gVehicleComponents[idx][0] != INVALID_VEHICLE_COMPONENT_ID);
}