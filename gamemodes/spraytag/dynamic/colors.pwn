stock st_FormatMessage( message[] )
{
    new _st_message[300];
    format( _st_message, sizeof( _st_message ), "%s", message );
    strreplace( _st_message,"{","#", true );
    if( strfind( _st_message, "(", true) != -1 )
    {
        strreplace( _st_message, "(n)","\n", true );
        new color_list = sizeof( st_colors ),
            color_replace[ 50 ];
        for( new i; i < color_list; i++ )
        {
            format( color_replace, sizeof( color_replace ),"{%s}", st_colors[ i ][ st_color_hex ] );
            strreplace( _st_message, st_colors[ i ][ st_color_code ], color_replace, true );
        }
    }
    return _st_message;
}

